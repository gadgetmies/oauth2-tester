{
  "name": "oauth2-tester",
  "version": "0.0.1",
  "description": "OAuth2 teser",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --ci --silent",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "aggregate-error": "^3.1.0",
    "axios": "^0.21.0",
    "axios-cookiejar-support": "^1.0.1",
    "base64url": "^3.0.1",
    "ramda": "^0.27.1",
    "tough-cookie": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owsas/typescript-module-template.git"
  },
  "bugs": {
    "url": "https://github.com/owsas/typescript-module-template/issues"
  },
  "homepage": "https://github.com/owsas/typescript-module-template#readme"
}
